{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "esModuleInterop": true,                           
    "forceConsistentCasingInFileNames": true,           
    "strict": false,                            
    "noImplicitThis": true,                          
    "sourceMap": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"]
    }
  }
}
